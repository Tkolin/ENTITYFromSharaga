<Page x:Class="ENTITYFromSharaga.AddEditClient"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ENTITYFromSharaga"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AddEditClient">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="Типо все поля тут должны быть"/>
            <TextBox Name="tbox"/>
            <Button x:Name="save" Click="save_Click" Content="Сохранить"/>
            <Button x:Name="back" Click="back_Click" Content="Назад"/>
        </StackPanel>
    </Grid>
</Page>
